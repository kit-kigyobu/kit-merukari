<div class="wrapper">
    <main class="container">
    <div class= "contents">
      <div class="cttHead">
            <h2>新規会員登録</h2>
      </div>
      <div class="mainForm">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="form-element-wrapper">
    <label for="UserEmail">メールアドレス</label>
    <%= f.email_field :email, autofocus: true, autocomplete: "email",:class=>"form-element form-field" %>
  </div>

  <div class="form-element-wrapper">
    <label for="UserPassword">パスワード</label>
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %>文字以上)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password", :class=>"form-element form-field" %>
  </div>

  <div class="form-element-wrapper">
    <label for="UserPassword">パスワード再入力</label>
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password", :class=>"form-element form-field" %>
  </div>

  <div class="actions">
    <%= f.submit "登録する", :class=>"form-element button button-Primary" %>
  </div>
<% end %>

<div class="loginhelp">
<%= render "devise/shared/links" %>
</div>
</div>

</main>
<footer class="pageFoot">
<div class="footer-bottom">
    <div class="kigyologo">
        <a href="/top"><img src="#" alt="OSAGARI"></a>
    </div>
    <div class="copyright">
        <small>&copy; <span>2019</span>京都学生起業部</small>
    </div>
</div>
</footer>
</div>
